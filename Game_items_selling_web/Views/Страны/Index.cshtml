@model IEnumerable<Game_items_selling_web.Models.Страны>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Страна)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Страна)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Буквенный_код_страны }) |
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Import to Excel", "Excel")