@model IEnumerable<Game_items_selling_web.Models.Возрастной_рейтинг>

@{
    ViewBag.Title = "Index";
}

<h2>Возрастные рейтинги</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Название_рейтинга)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Рейтинг)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Название_рейтинга)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Рейтинг)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Код_возрастного_рейтинга }) |
                @Html.ActionLink("Details", "Details", new { id = item.Код_возрастного_рейтинга }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Код_возрастного_рейтинга })
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Import to Excel", "Excel")
