@model IEnumerable<Game_items_selling_web.Models.Жанры>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Жанр)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Жанр)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Код_жанра }) |
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Import to Excel", "Excel")
