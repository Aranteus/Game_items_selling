@model IEnumerable<Game_items_selling_web.Models.Издатели>

@{
    ViewBag.Title = "Index";
}

<h2>Издатели</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Издатель)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Страны.Страна)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
